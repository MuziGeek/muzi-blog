    "target": "ESNext",
    "module": "CommonJS",
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "types": ["jquery","fancybox"],
    "skipLibCheck": true
  },
  "typeAcquisition": {
    "enable": true
  },
  "references": [
    {
      "path": "./source"
    },
    {
      "path": "./scripts"
    }
  ]
}
